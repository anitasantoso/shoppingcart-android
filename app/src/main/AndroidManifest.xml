        </activity>
        <activity
            android:name="au.com.exercise.shoppingcart.activity.MainActivity_"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="au.com.exercise.shoppingcart.activity.ShoppingCartActivity_" >
        </activity>
        <activity
            android:name="au.com.exercise.shoppingcart.activity.ProductDetailActivity_"
            android:label="@string/title_activity_product_detail" >
        </activity>
    </application>

</manifest>
